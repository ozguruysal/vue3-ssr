{
  "name": "vue3-ssr",
  "version": "1.0.0",
  "description": "Vue3 + Server Side Rendering",
  "main": "index.js",
  "repository": "https://github.com/ozguruysal/vue3-ssr.git",
  "author": "Özgür Uysal",
  "license": "MIT",
  "scripts": {
    "dev": "node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV=production node server",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.js --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js --hide-modules",
    "lint": "eslint 'src/**/*.{js,vue}'"
  },
  "dependencies": {
    "@vue/server-renderer": "^3.0.0-beta.13",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "serve-favicon": "^2.5.0",
    "tailwindcss": "^1.4.6",
    "vue": "^3.0.0-beta.13",
    "vue-bundle-renderer": "^0.0.3",
    "vue-router": "^4.0.0-alpha.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@vue/compiler-sfc": "^3.0.0-beta.13",
    "autoprefixer": "^9.8.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-alpha.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "hash-sum": "^2.0.0",
    "lodash.uniq": "^4.5.0",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "vue-loader": "^16.0.0-beta.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
